{
    "name": "markup-load",
    "version": "1.0.0",
    "description": "노드에서 html 파일 합치기",
    "main": "index.js",
    "scripts": {
        "html": "nodemon -e html --watch htmls/ index.js",
        "prettier": "npx prettier --write htmls/*/*.html",
        "prettier-check": "npx prettier --check htmls/*/*.html",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "thegi.code@gmail.com",
    "license": "ISC",
    "devDependencies": {
        "nodemon": "^2.0.15",
        "prettier": "2.6.2"
    },
    "nodemonConfig": {
        "ignore": [
            "backup/*",
            "docs/*",
            "htmls/page/*"
        ]
    }
}
